<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping> 
    <class name="com.addrecord.model.addrecordVO" table="addrecord">
        <id name="id" column="id"> 
            <generator class="increment"/> 
        </id>
        <property name="storedatetime" column="storedatetime"/>
        <property name="coin10" column="coin10"/>
        <property name="coin50" column="coin50"/>
        <property name="paper100" column="paper100"/>
        <property name="paper500" column="paper500"/>
        <property name="paper1000" column="paper1000"/>
        <property name="point" column="point"/>
        <property name="errorcode" column="errorcode"/>
        <property name="username" column="username"/>
        <property name="deviceid" column="deviceid"/>
        <property name="storeid" column="storeid"/>
        <property name="cardid" column="cardid"/>
        <property name="name" column="name"/>
        <property name="city" column="city"/>

    </class>
</hibernate-mapping>

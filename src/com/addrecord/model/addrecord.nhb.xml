<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping> 
    <class name="com.device.model.deviceVO" table="device">
        <id name="did" column="did"> 
            <generator class="increment"/> 
        </id>
        <property name="number" column="number"/>
        <property name="coin" column="coin"/>
        <property name="paper" column="paper"/>
        <property name="location" column="location"/>
        <property name="refund" column="refund"/>
        <property name="uid" column="uid"/>
        <property name="status" column="status"/>
        <property name="error" column="error"/>
        <property name="machid" column="machid"/>
        <property name="freecount" column="freecount"/>
        <property name="freecountset" column="freecountset"/>

    </class>
</hibernate-mapping>
